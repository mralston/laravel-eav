{
  "name": "mralston/laravel-eav",
  "description": "Implements the Entity-Attribute-Value pattern to supplement Eloquent with adhoc attributes.",
  "keywords": [
    "mralston",
    "laravel",
    "entity attribute value"
  ],
  "version": "1.0.0",
  "homepage": "https://github.com/mralston/laravel-eav",
  "license": "MIT",
  "authors": [
    {
      "name": "Matt Ralston",
      "email": "matt@mralston.co.uk",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/database": "^10.0",
    "illuminate/support": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Mralston\\Eav\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mralston\\Eav\\Providers\\EavServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
